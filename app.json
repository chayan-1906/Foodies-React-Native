{
  "expo": {
    "name": "Foodies",
    "slug": "foodies-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "foodiesexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chayan1906.foodiesexpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.chayan1906.foodiesexpo"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "/assets/fonts/*"
    ]
  }
}
